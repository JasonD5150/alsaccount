<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<constant name="struts.devMode" value="false" />

	<package name="fwp.alsaccount.sabhrs" namespace="" extends="struts-default">
      <action name="transGroupMaint_*" method="{1}" class="fwp.alsaccount.action.GenericAction">
            <result>sabhrs/transactionGroupMaint.jsp</result>
            <result name="error">sabhrs/transactionGroupMaint.jsp</result>
            <result name="input">sabhrs/transactionGroupMaint.jsp</result>         
      </action>
      <action name="uploadSabhrsToSum_*" method="{1}" class="fwp.alsaccount.action.GenericAction">
            <result>sabhrs/uploadSabhrsToSummary.jsp</result>
            <result name="error">sabhrs/uploadSabhrsToSummary.jsp</result>
            <result name="input">sabhrs/uploadSabhrsToSummary.jsp</result>         
      </action>    	
      <action name="genNonAlsEntries_*" method="{1}" class="fwp.alsaccount.sabhrs.action.GenNonAlsEntriesAction">
            <result>sabhrs/genNonAlsEntries.jsp</result>
            <result name="error">sabhrs/genNonAlsEntries.jsp</result>
            <result name="input">sabhrs/genNonAlsEntries.jsp</result>         
      </action>
      <action name="accMasterDialog_*" method="{1}" class="fwp.alsaccount.action.GenericAction">
            <result>sabhrs/include/nonAlsMasterDialog.jsp</result>
            <result name="error">sabhrs/include/nonAlsMasterDialog.jsp</result>
            <result name="input">sabhrs/include/nonAlsMasterDialog.jsp</result>         
      </action>
      <action name="transGroupApproval_*" method="{1}" class="fwp.alsaccount.sabhrs.action.TransactionGroupApprovalAction">
            <result>sabhrs/transactionGroupApproval.jsp</result>
            <result name="error">sabhrs/transactionGroupApproval.jsp</result>
            <result name="input">sabhrs/transactionGroupApproval.jsp</result>         
      </action>
      <action name="getTreasureDepositTicketPdf_*" method="{1}" class="fwp.alsaccount.sabhrs.action.GetTreasureDepositTicketAction">
               <result name="success" type="stream">
                <param name="contentType">application/pdf</param>
                <param name="contentDisposition">attachment;filename="${fileName}"</param>
                <param name="inputName">inputStream</param>
            </result>
            <result name="error">sabhrs/transactionGroupApproval.jsp</result>
        </action>
        <action name="transGroupMassApproval_*" method="{1}" class="fwp.alsaccount.action.GenericAction">
			<result>sabhrs/transactionGroupMassApproval.jsp</result>
			<result name="error">sabhrs/transactionGroupMassApproval.jsp</result>
			<result name="input">sabhrs/transactionGroupMassApproval.jsp</result>         
		</action>
	</package>
 
 	<package name="fwp.alsaccount.sabhrs.grid" namespace="/alsAccount" extends="json-default">     
		<action name="transGroupMaintGrid_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGroupGridAction">
            <result type="json" />
        </action>       
        <action name="transGroupMaintGridEdit_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGroupGridEditAction">
			<result type="json"></result>
            <result type="json" name="input"></result>
            <result name="error_json" type="json" >
               <param name="ignoreHierarchy">false</param>
            </result>
		</action>
		<action name="transGroupDtlsGrid_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGrpStatusGridAction">
            <result type="json" />
        </action>       
        <action name="transGroupDtlsGridEdit_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGrpStatusGridEditAction">
			<result type="json"></result>
            <result type="json" name="input"></result>
            <result name="error_json" type="json" >
               <param name="ignoreHierarchy">false</param>
            </result>
		</action>
		<action name="alsSabhrsEntriesGrid_*" class="fwp.alsaccount.sabhrs.grid.AlsSabhrsEntriesGridAction">
            <result type="json" />
        </action>
         <action name="alsSabhrsEntriesGridEdit_*" class="fwp.alsaccount.sabhrs.grid.AlsSabhrsEntriesGridEditAction">
			<result type="json"></result>
            <result type="json" name="input"></result>
            <result name="error_json" type="json" >
               <param name="ignoreHierarchy">false</param>
            </result>
		</action> 
		<action name="transGroupApprovalGrid_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGrpApprovalGridAction">
            <result type="json" />
        </action> 
        <action name="transGroupApprovalGridEdit_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGrpApprovalGridEditAction">
			<result type="json"></result>
            <result type="json" name="input"></result>
            <result name="error_json" type="json" >
               <param name="ignoreHierarchy">false</param>
            </result>
		</action>  
		<action name="transGroupMassApprovalGrid_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGrpMassApprovalGridAction">
			<result type="json" />
		</action> 
		<action name="transGroupMassApprovalGridEdit_*" class="fwp.alsaccount.sabhrs.grid.AlsTransactionGrpMassApprovalGridEditAction">
			<result type="json"></result>
			<result type="json" name="input"></result>
			<result name="error_json" type="json" >
			   <param name="ignoreHierarchy">false</param>
			</result>
		</action>     
   </package>

	<package name="fwp.alsaccount.sabhrs.json" extends="json-default" >
		<action  name="uploadSabhrsToSumJson" class="fwp.alsaccount.sabhrs.json.UploadSabhrsEntriesToSumJson" >
            <result name="success" type="json" />
            <result name="input" type="json"></result>
            <result name="error_json" type="json" >
               <param name="ignoreHierarchy">false</param>
            </result>
        </action>
    </package>

</struts>    
